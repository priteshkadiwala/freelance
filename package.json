    "core-js": "^3.6.4",
    "express": "^4.17.1",
    "gulp": "^3.9.1",
    "gulp-clean": "^0.3.2",
    "gulp-install": "^1.1.0",
    "gulp-open": "^2.1.0",
    "gulp-postcss": "^8.0.0",
    "gulp-sass": "^3.1.0",
    "gulp-sourcemaps": "^2.6.3",
    "lodash": "^4.17.20",
    "serve-static": "^1.14.1",
    "vue": "^2.6.11",
    "vue-lazyload": "^1.3.3",
    "vue-router": "^3.1.6",
    "vuex": "^3.1.3"
